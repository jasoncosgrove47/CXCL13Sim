		<PERSISTENCE>0.9</PERSISTENCE><!-- was 0.40, bias of the memory vector, values less than 1 means memory is stronger than current signal -->
	</BC>

	<ODE> 
		<LR>0</LR><!-- 3000 -->
		<Rf>50000</Rf><!--30000 -->
		<Ri>0</Ri>
